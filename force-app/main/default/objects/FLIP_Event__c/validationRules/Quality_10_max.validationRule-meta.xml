<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Quality_10_max</fullName>
    <active>true</active>
    <errorConditionFormula>OR(Quality__c &gt; 10,Quality__c &lt; 0)</errorConditionFormula>
    <errorDisplayField>Quality__c</errorDisplayField>
    <errorMessage>Quality score must be between 0 and 10</errorMessage>
</ValidationRule>
