<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Temp_LMS_Start_Date_Programme</fullName>
    <active>false</active>
    <errorConditionFormula>AND(
  Temp_LMS_Start_Date__c &lt; Programme__r.LMS_End_Date__c,
  NOT ISBLANK(Temp_LMS_Start_Date__c)
)</errorConditionFormula>
    <errorDisplayField>Temp_LMS_Start_Date__c</errorDisplayField>
    <errorMessage>The temporary Canvas start date can&#39;t be less than the programme Canvas end date</errorMessage>
</ValidationRule>
